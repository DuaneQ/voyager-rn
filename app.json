{
  "expo": {
    "name": "TravalPass",
    "slug": "voyager-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travalpass.app",
      "buildNumber": "15",
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default"
        ]
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload profile and gallery photos.",
        "NSCameraUsageDescription": "This app needs access to your camera to take profile and gallery photos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save photos to your photo library.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.533074391000-quot684rc8kugrni3eh2c6bsq3u5rcqs"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.533074391000-quot684rc8kugrni3eh2c6bsq3u5rcqs"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.travalpass.app",
      "versionCode": 5,
      "googleServicesFile": "./google-services.json",
      "icon": "./AppIcons/playstore.png",
      "softwareKeyboardLayoutMode": "resize",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "scheme": "travalpass",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "./plugins/withGooglePlacesAndroid",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "6fc90234-4d23-427c-918f-75d141efe8ed"
      }
    }
  }
}
