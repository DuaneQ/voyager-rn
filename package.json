{
  "name": "voyager-rn",
  "version": "1.0.0",
  "description": "React Native Expo version of voyager-pwa with identical functionality",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "# === APP LAUNCH ===": "",
    "start": "expo start",
    "start:android": "expo run:android",
    "start:ios": "expo run:ios",
    "start:web": "expo start --web",
    "# === UNIT TESTS ===": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "# === E2E TESTS - iOS ===": "",
    "e2e:ios": "node automation/scripts/run-ios-e2e.js",
    "e2e:ios:headless": "HEADLESS=true node automation/scripts/run-ios-e2e.js",
    "e2e:ios:3x": "node automation/scripts/repeat-run.js 3 ios",
    "e2e:ios:headless:3x": "HEADLESS=true node automation/scripts/repeat-run.js 3 ios",
    "# === E2E TESTS - Android ===": "",
    "e2e:android": "node automation/scripts/run-android-e2e.js",
    "e2e:android:headless": "HEADLESS=true node automation/scripts/run-android-e2e.js",
    "e2e:android:3x": "node automation/scripts/repeat-run.js 3 android",
    "e2e:android:headless:3x": "HEADLESS=true node automation/scripts/repeat-run.js 3 android",
    "# === E2E TESTS - CI/CD Ready ===": "",
    "e2e:all:headless": "npm run e2e:ios:headless && npm run e2e:android:headless",
    "e2e:all:headless:3x": "npm run e2e:ios:headless:3x && npm run e2e:android:headless:3x",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.3",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.2",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/bottom-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.9",
    "expo": "~51.0.28",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.28",
    "expo-video-thumbnails": "~8.0.0",
    "firebase": "^10.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.2.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.1.5",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.79",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "msw": "^1.2.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
