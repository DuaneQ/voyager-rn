{
  "name": "voyager-rn",
  "version": "1.0.0",
  "description": "React Native Expo version of voyager-pwa with identical functionality",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "# === APP LAUNCH ===": "",
    "start": "expo start",
    "start:android": "expo run:android",
    "start:ios": "expo run:ios",
    "start:web": "expo start --web",
    "postinstall": "patch-package",
    "# === UNIT TESTS ===": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "# === INTEGRATION TESTS ===": "",
    "test:integration": "./scripts/run-integration-tests.sh",
    "test:all": "npm test && npm run test:integration",
    "import-openflights": "node scripts/import-openflights.js",
    "# === E2E TESTS - iOS ===": "",
    "e2e:ios": "node automation/scripts/run-ios-e2e.js",
    "e2e:ios:local": "node automation/scripts/run-ios-e2e.js",
    "e2e:ios:ci": "CI=1 node automation/scripts/run-ios-e2e.js",
    "e2e:ios:headless": "HEADLESS=true node automation/scripts/run-ios-e2e.js",
    "e2e:ios:3x": "node automation/scripts/repeat-run.js 3 ios",
    "e2e:ios:headless:3x": "HEADLESS=true node automation/scripts/repeat-run.js 3 ios",
    "# === E2E TESTS - Android ===": "",
    "e2e:android": "node automation/scripts/run-android-e2e.js",
    "e2e:android:headless": "HEADLESS=true node automation/scripts/run-android-e2e.js",
    "e2e:android:3x": "node automation/scripts/repeat-run.js 3 android",
    "e2e:android:headless:3x": "HEADLESS=true node automation/scripts/repeat-run.js 3 android",
    "# === E2E TESTS - CI/CD Ready ===": "",
    "e2e:all:headless": "npm run e2e:ios:headless && npm run e2e:android:headless",
    "e2e:all:headless:3x": "npm run e2e:ios:headless:3x && npm run e2e:android:headless:3x",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.3.29",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "expo": "~54.0.24",
    "expo-apple-authentication": "~8.0.8",
    "expo-av": "~16.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.18",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.15",
    "expo-video-thumbnails": "~10.0.7",
    "firebase": "^12.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.50.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@react-native-community/cli": "^20.0.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "firebase-admin": "^12.7.0",
    "firebase-tools": "^14.25.1",
    "jest": "~29.7.0",
    "jest-circus": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "msw": "^1.2.1",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.1",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
